<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
    <test name="Samsung Galaxy S23 Ultra">
        <parameter name="deviceName" value="OnePlus 8"/>
        <parameter name="deviceVersion" value="10.0"/>
        <parameter name="platform" value="android"/>
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="com.qm.android.testcases.HomePageTest">
                <methods>
                    <include name="addContactAndVerifyContactDetails"/>
                </methods>
            </class>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->